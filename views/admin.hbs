      <h2>Insert Lost Item</h2>

      <form method="POST">
      <label for="item_name">Item Found</label><br>
      <input type="text" id="item_name" name="item_name"><br>

      <label for="item_category">Category</label><br>
      <input type="text" id="item_category" name="item_category"><br>

      <label for="item_value">Approx Value</label><br>
      <input type="text" id="item_value" name="item_value"><br>

      <label for="item_desc">Description</label><br>
      <textarea type="text" id="item_desc" name="item_desc"></textarea><br>

      <label for="item_location">Location Found</label><br>
      <input type="item_location" id="item_location" name="lname">
      
      <input type="checkbox" id="outside" name="outside" value="Outside">
      <label for="vehicle1">Found Outside</label><br>

      <label for="item_room">Location Room</label><br>
      <input type="text" id="item_room" name="item_room"><br>

      <label for="found_by">Found by ID</label><br>
      <input type="text" id="found_by" name="found_by"><br>

      <label for="found_by_desc">Found by Description: Full Name,School Id,Phone #</label><br>
      <input type="text" id="found_by_desc" name="found_by_desc"><br>

      <label for="date_found">Date Found</label><br>
      <input type="date" id="date_found" name="date_found"><br>
      
      <button type="button" class="btn btn-primary" onClick="insertNew()">Testing Button for insert new</button>
      </form>


       <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script> 

